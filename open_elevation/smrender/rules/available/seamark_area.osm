<?xml version='1.0' encoding='UTF-8'?>
<osm version='0.6' generator="Bernhard R. Fischer">

   <way>
      <tag k='man_made' v='pier'/>
      <tag k='_action_' v='draw:color=navajowhite;ignore_open=1'/>
   </way>
   <way>
      <tag k='man_made' v='pier'/>
      <tag k='_action_' v='draw:bcolor=black'/>
   </way>

   <way>
      <tag k='man_made' v='breakwater'/>
      <tag k='_action_' v='draw:color=black'/>
   </way>

   <!-- direction of traffic flow INT1 IM 10 -->
   <way version="-10">
      <tag k='seamark:type' v='separation_lane'/>
      <tag k='_action_' v='ins_eqdist:distance=10mm'/>
   </way>
   <node>
      <tag k='seamark:type' v='separation_lane'/>
      <tag k='generator' v='smrender'/>
      <tag k='_action_' v='img:file=SeaMapSymbols/LaneArrow.svg;anglekey=bearing'/>
   </node>

   <!-- separation zone INT1 IM 13 -->
   <way>
      <tag k='seamark:type' v='separation_zone'/>
      <tag k='_action_' v='draw:color=#40FF00FF'/>
   </way>
   
   <!-- submarine cable INT1 IL 30.1 -->
   <way>
      <tag k='seamark:type' v='cable_submarine'/>
      <tag k='_action_' v='draw:color=magenta;wavy=yes'/>
   </way>

   <!-- pipeline INT1 IL 40.1, IL 40.2 -->
   <way>
      <tag k='seamark:type' v='pipeline_submarine'/>
      <tag k='_action_' v='draw:bcolor=magenta;bstyle=pipe;bwidth=0.2'/>
   </way>

   <!-- offshore platform INT1 IL 10 -->
   <node version="-10">
      <tag k='seamark:type' v='platform'/>
      <tag k='_action_' v='shape:style=circle;radius=.3'/>
      </node>
   <node version="-10">
      <tag k='seamark:type' v='platform'/>
      <tag k='_action_' v='shape:style=square;radius=1.5;angle=45'/>
      </node>
   <way>
      <tag k='seamark:type' v='platform'/>
      <tag k='_action_' v='draw:bcolor=black'/>
   </way>

   <!-- anchorage area INT1 IN 12.1 -->
   <way version="-10">
      <tag k='seamark:type' v='anchorage'/>
      <tag k='_action_' v='ins_eqdist:distance=1.5'/>
   </way>
   <node>
      <tag k='seamark:type' v='anchorage'/>
      <tag k='generator' v='smrender'/>
      <tag k='_action_' v='img:file=SeaMapSymbols/Anchorage.svg;anglekey=bearing;angle=-90'/>
   </node>
   <!-- separation line INT1 IM 15, IN 1.2 -->
   <way>
      <tag k='seamark:type' v='/separation_boundary|anchorage/'/>
      <tag k='_action_' v='draw:bcolor=#40FF00FF;bwidth=0.5;bstyle=dashed'/>
   </way>

</osm>

